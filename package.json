{
  "name": "wordpress-boilerplate",
  "version": "6.0.2",
  "author": "Dots United GmbH",
  "license": "MIT",
  "scripts": {
    "build": "rimraf web/wp-content/themes/wordpress-boilerplate/assets/scripts && webpack",
    "watch": "rimraf web/wp-content/themes/wordpress-boilerplate/assets/scripts && webpack --watch --progress",
    "build-modernizr": "modernizr -c modernizr-config.json -d assets/main/core/modernizr.js",
    "favicon-generate": "real-favicon generate assets/favicon/real-favicon-description.json assets/favicon/real-favicon-data.json web/favicons && cp web/favicons/favicon.ico web/favicon.ico && touch web/favicons/favicons.html && real-favicon inject assets/favicon/real-favicon-data.json web/favicons web/favicons/favicons.html",
    "favicon-check": "real-favicon check-for-update --fail-on-update assets/favicon/real-favicon-data.json"
  },
  "dependencies": {
    "dotsunited-equal-height-blocks": "3.1.0",
    "dotsunited-off-canvas-navigation": "2.4.0",
    "dotsunited-sliding-navigation": "2.1.1",
    "fontfaceobserver": "1.7.3",
    "gee.less": "3.0.0",
    "jquery": "1.12.4",
    "modernizr": "3.3.1",
    "slick-carousel": "1.6.0"
  },
  "devDependencies": {
    "cli-real-favicon": "0.0.6",
    "css-loader": "0.23.1",
    "exports-loader": "0.6.3",
    "extend": "3.0.0",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.9.0",
    "grunt": "1.0.1",
    "grunt-contrib-clean": "1.0.0",
    "grunt-contrib-imagemin": "1.0.1",
    "grunt-webpack": "1.0.11",
    "image-webpack-loader": "1.8.0",
    "imports-loader": "0.6.5",
    "less": "2.7.1",
    "less-loader": "2.2.3",
    "postcss-cssnext": "2.7.0",
    "postcss-focus": "1.0.0",
    "postcss-loader": "0.9.1",
    "postcss-reporter": "1.4.1",
    "raw-loader": "0.5.1",
    "rimraf": "2.5.4",
    "style-loader": "0.13.1",
    "url-loader": "0.5.7",
    "webpack": "1.13.1"
  }
}
